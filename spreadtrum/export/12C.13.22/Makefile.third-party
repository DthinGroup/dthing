#SUN JAVA lib config
ifeq ($(strip $(JAVA_SUPPORT)), SUN)
    ifeq ($(strip $(STACK_CHECK_SUPPORT)), TRUE)
      ifeq ($(strip $(PLATFORM)),SC8800H)
              ifeq ($(strip $(CHIP_ENDIAN)), LITTLE)
                3RD_LIBS      +=Third-party/Sun_Java/arm/arm9/normal_le/jvm.a
              else
                3RD_LIBS      +=Third-party/Sun_Java/arm/arm9/normal/jvm.a
              endif
      else
         ifeq ($(strip $(PLATFORM)),SC6800H)
      ifeq ($(strip $(CHIP_ENDIAN)), LITTLE)
        3RD_LIBS      +=Third-party/Sun_Java/arm/arm9/le/jvm.a
      else
        3RD_LIBS      +=Third-party/Sun_Java/arm/arm9/normal/jvm.a
      endif
         else
             ifeq ($(strip $(PLATFORM)),SC8800G)
                ifeq ($(strip $(CHIP_ENDIAN)), LITTLE)
                  3RD_LIBS      +=Third-party/Sun_Java/arm/arm9/normal_le/jvm.a
                else
                  3RD_LIBS      +=Third-party/Sun_Java/arm/arm9/normal/jvm.a
                endif
             else
                3RD_LIBS      +=Third-party/Sun_Java/arm/arm7/jvm.a
             endif
         endif
      endif
    else
        ifeq ($(strip $(PLATFORM)),SC8800H)
            3RD_LIBS      +=Third-party/Sun_Java/arm/arm9/nostack/jvm.a
        else
              ifeq ($(strip $(PLATFORM)),SC6800H)
                    3RD_LIBS     +=Third-party/Sun_Java/arm/arm9/nostack/jvm.a
              else
                ifeq ($(strip $(PLATFORM)),SC8800G)
                    ifeq ($(strip $(CHIP_ENDIAN)), LITTLE)
                      3RD_LIBS      +=Third-party/Sun_Java/arm/arm9/nostack_le/jvm.a
                    else
                      3RD_LIBS      +=Third-party/Sun_Java/arm/arm9/nostack/jvm.a
                    endif
                else
                 3RD_LIBS     +=Third-party/Sun_Java/arm/arm7/jvm.a
                endif
            endif
        endif
    endif  
endif 

