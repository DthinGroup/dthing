				3RD_LIBS += Third-party/TencentMcareV31/Arm_lib/TencentMcareV31_No_Stack_Debug.a
				else
				3RD_LIBS += Third-party/TencentMcareV31/Arm_lib/TencentMcareV31_No_Stack.a				
				endif
			endif
		endif
endif
