# multilanguage config for cstar
ifeq ($(strip $(IM_ENGINE)), CSTAR)
  ifeq ($(strip $(COMPILER)), RVCT)
    ifeq ($(strip $(CHIP_ENDIAN)), LITTLE)
        ifeq ($(strip $(IM_SIMP_CHINESE_SUPPORT)), TRUE)
          3RD_LIBS           =Third-party/cstar/Arm_lib/rvct_le/csk2_full.a
        else
           ifeq ($(strip $(IM_TRAD_CHINESE_SUPPORT)), TRUE)
              3RD_LIBS           =Third-party/cstar/Arm_lib/rvct_le/csk2_full.a  
            else
              3RD_LIBS           =Third-party/cstar/Arm_lib/rvct_le/csk2_latin.a
            endif 
        endif               
    else 
        ifeq ($(strip $(IM_SIMP_CHINESE_SUPPORT)), TRUE)
           3RD_LIBS           =Third-party/cstar/Arm_lib/rvct3.1/csk2_full.a
        else
            ifeq ($(strip $(IM_TRAD_CHINESE_SUPPORT)), TRUE)
              3RD_LIBS           =Third-party/cstar/Arm_lib/rvct3.1/csk2_full.a  
            else
              3RD_LIBS           =Third-party/cstar/Arm_lib/rvct3.1/csk2_latin.a
            endif 
        endif
    endif
  else
    ifeq ($(strip $(STACK_CHECK_SUPPORT)), TRUE)
        ifeq ($(strip $(CHIP_ENDIAN)), LITTLE)
            ifeq ($(strip $(IM_SIMP_CHINESE_SUPPORT)), TRUE)
              3RD_LIBS           =Third-party/cstar/Arm_lib/LE/csk2_full.a
            else
               ifeq ($(strip $(IM_TRAD_CHINESE_SUPPORT)), TRUE)
                  3RD_LIBS           =Third-party/cstar/Arm_lib/LE/csk2_full.a  
                else
                  3RD_LIBS           =Third-party/cstar/Arm_lib/LE/csk2_latin.a
                endif 
            endif               
        else 
            ifeq ($(strip $(IM_SIMP_CHINESE_SUPPORT)), TRUE)
               3RD_LIBS           =Third-party/cstar/Arm_lib/normal/csk2_full.a
            else
                ifeq ($(strip $(IM_TRAD_CHINESE_SUPPORT)), TRUE)
                  3RD_LIBS           =Third-party/cstar/Arm_lib/normal/csk2_full.a  
                else
                  3RD_LIBS           =Third-party/cstar/Arm_lib/normal/csk2_latin.a
                endif 
            endif
        endif
    else
        ifeq ($(strip $(CHIP_ENDIAN)), LITTLE)
            ifeq ($(strip $(IM_SIMP_CHINESE_SUPPORT)), TRUE)
              3RD_LIBS           =Third-party/cstar/Arm_lib/nostack_le/csk2_full.a
            else
               ifeq ($(strip $(IM_TRAD_CHINESE_SUPPORT)), TRUE)
                 3RD_LIBS           =Third-party/cstar/Arm_lib/nostack_le/csk2_full.a  
               else
                 3RD_LIBS           =Third-party/cstar/Arm_lib/nostack_le/csk2_latin.a
               endif 
            endif               
        else 
            ifeq ($(strip $(IM_SIMP_CHINESE_SUPPORT)), TRUE)
              3RD_LIBS           =Third-party/cstar/Arm_lib/nostack/csk2_full.a
            else
                ifeq ($(strip $(IM_TRAD_CHINESE_SUPPORT)), TRUE)
                  3RD_LIBS           =Third-party/cstar/Arm_lib/nostack/csk2_full.a 
                else
                  3RD_LIBS           =Third-party/cstar/Arm_lib/nostack/csk2_latin.a
                endif 
            endif
        endif
    endif
  endif
endif
