# IA JAVA lib config
ifeq ($(strip $(JAVA_SUPPORT)), IA)
   IA_JAVA_PATH:=
   ifeq ($(strip $(SPRD_CUSTOMER)), FALSE)
      IA_JAVA_PATH:=Third-party/formal/ia_java/Arm_lib
   else
      IA_JAVA_PATH:=Third-party/ia_java/Arm_lib
   endif
   
   ifeq ($(strip $(CHIP_ENDIAN)), LITTLE)
    IA_JAVA_PATH:=$(strip $(IA_JAVA_PATH))/le
   endif
   
   ifeq ($(strip $(STACK_CHECK_SUPPORT)), TRUE)
      IA_JAVA_PATH:=$(strip $(IA_JAVA_PATH))/normal
   else
      IA_JAVA_PATH:=$(strip $(IA_JAVA_PATH))/nostack
   endif
   
   ifeq ($(strip $(SRAM_SIZE)), 32MBIT)
         IA_JAVA_PATH:=$(strip $(IA_JAVA_PATH))/reduce
   endif

   3RD_LIBS   += $(strip $(IA_JAVA_PATH))/ajsc_arm_rvct.a \
    $(strip $(IA_JAVA_PATH))/libmjb2.a \
    $(strip $(IA_JAVA_PATH))/libaicrypto.a
endif

